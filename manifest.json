{

  "manifest_version": 2,
  "name": "Savior",
  "version": "1.0",

  "description": "A devtool panel for easy sharing of requests, failed or otherwise",

  "permissions": [
    "downloads",
    "clipboardWrite"
  ],

  "background": {
    "scripts": [
      "devtools/browser-polyfill.js",
      "background/script.js"
    ]
  },

  "devtools_page": "devtools/requests.html"
}
